#include <iostream>
using namespace std;
int main()
{
  int n,a,b,c,i,j,m;
  cout<<"请输入一个不超过3位数的整数"<<endl;
  cin>>n;
  a=n/100;
  b=n/10%10;
  c=n%10;
  if(c!=0)
  {
    for(i=0;i<a;i++)
    cout<<"B";
    for(j=0;j<b;j++)
    cout<<"S";
    for(m=1;m<=c;m++)
    cout<<m;
  }
 if(c==0)
  {
	cout<<"不符合题目要求的个位数大于0，请重新输入一个数"<<endl;
  }
}
