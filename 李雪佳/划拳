#include<iostream>
using namespace std;
int main()
{
	int n,x1,x2,x3,x4,i,m1=0,m2=0,t1=0,t2=0;
	cin>>n;
	for(i=0;i<n;i++)
			cin>>x1>>x2>>x3>>x4;
	for(i=0;i<n;i++)
	{t1=(x2==(x1+x3));
	 t2=(x4==(x1+x3));
			 if(t1!=t2)
			 {
			 if(t1)
				m1++;
	        else  
				m2++;
			 }}
			cout<<m1<<" "<<m2;
			return 0;
}
