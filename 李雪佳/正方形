#include<iostream>
using namespace std;
int main()
{
	int n;
	char a;
	cin>>n;
	cin>>a;
	for(int i=0;i<n;i++)
		cout<<a;
	    cout<<endl;
   for(int i=0;i<4;i++)
		{cout<<a;
   for(int i=0;i<n-2;i++)
	    cout<<" ";
        cout<<a;
        cout<<endl;}
   for(int i=0;i<n;i++)
		cout<<a;
   return 0;
}
