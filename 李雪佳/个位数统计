#include<iostream>
using namespace std;
int main()
{
	int x[100],k,t1=0,t2=0,t3=0,t4=0,t5=0,t6=0,t7=0,t8=0,t9=0,t=0,x1,x2,x3,x4,x5,x6,x7,x8,x9,x0;
	cin>>k;
	for(int i=0;i<k;i++)
		cin>>x[i];
	for(int i=0;i<k;i++)
	{if(x[i]==0)
	       { x1=i;
	       t1++;
	      continue;}
	if(x[i]==1)
		   { x2=i;
	       t2++;
	continue;}
	if(x[i]==2)
		  { x3=i;
	       t3++;
	continue;}
	if(x[i]==3)
		  { x4=i;
	       t4++;
	continue;}
	if(x[i]==4)
	   	  { x5=i;
	      t5++;
	continue;}
	if(x[i]==5)
		  { x6=i;
	      t6++;
	continue;}
	if(x[i]==6)
		  { x7=i;
	       t7++;
	continue;}
	if(x[i]==7)
	   	 { x8=i;
	     t8++;
	continue;}
	if(x[i]==8)
			{ x9=i;
	       t9++;
	continue;}
	if(x[i]==9)
		 {x0=i;
	     t++;
	continue;}}
	cout<<x[x1]<<":"<<t1<<endl;
	cout<<x[x2]<<":"<<t2<<endl;
	cout<<x[x3]<<":"<<t3<<endl;
	cout<<x[x4]<<":"<<t4<<endl;
	cout<<x[x5]<<":"<<t5<<endl;
	cout<<x[x6]<<":"<<t6<<endl;
	cout<<x[x7]<<":"<<t7<<endl;
	cout<<x[x8]<<":"<<t8<<endl;
	cout<<x[x9]<<":"<<t9<<endl;
	cout<<x[x0]<<":"<<t<<endl;
return 0;
}
